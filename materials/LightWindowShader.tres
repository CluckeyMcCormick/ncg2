void light() {
// ColorUniform:3
	vec3 n_out3p0 = dark_color.rgb;
	float n_out3p1 = dark_color.a;

// Input:21
	vec3 n_out21p0 = LIGHT_COLOR;

// Input:8
	vec3 n_out8p0 = NORMAL;

// Input:2
	vec3 n_out2p0 = LIGHT;

// DotProduct:7
	float n_out7p0 = dot(n_out8p0, n_out2p0);

// VectorClamp:12
	vec3 n_in12p1 = vec3(0.00000, 0.00000, 0.00000);
	vec3 n_in12p2 = vec3(1.00000, 1.00000, 1.00000);
	vec3 n_out12p0 = clamp(vec3(n_out7p0), n_in12p1, n_in12p2);

// Input:10
	vec3 n_out10p0 = ATTENUATION;

// VectorOp:13
	vec3 n_out13p0 = n_out12p0 * n_out10p0;

// VectorMix:16
	vec3 n_out16p0 = mix(n_out3p0, n_out21p0, n_out13p0);

// ColorUniform:23
	vec3 n_out23p0 = window_color.rgb;
	float n_out23p1 = window_color.a;

// TextureUniform:18
	vec3 n_out18p0;
	float n_out18p1;
	{
		vec4 n_tex_read = texture(TextureUniform, UV.xy);
		n_out18p0 = n_tex_read.rgb;
		n_out18p1 = n_tex_read.a;
	}

// VectorOp:22
	vec3 n_out22p0 = n_out23p0 * n_out18p0;

// VectorScalarMix:19
	vec3 n_out19p0 = mix(n_out16p0, n_out22p0, n_out18p1);

// Input:24
	vec3 n_out24p0 = DIFFUSE_LIGHT;

// ColorOp:25
	vec3 n_out25p0 = max(n_out19p0, n_out24p0);

// Output:0
	DIFFUSE_LIGHT = n_out25p0;

}
